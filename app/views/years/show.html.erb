<h2><%= @program.name %>, <%= @year.year %></h2>

<h3>Year ID: <%= @year.id %></h3>
<h3>Program ID: <%= @program.id %>, </h3>
<div>
	<ul class="nav nav-tabs">
		<% @tabs.each do |tab| %>
			<li><%= link_to tab.name %></li>
		<% end %>
	</ul>
	<div class="tab-content">

	<% @tabs.each do |tab| %>
		<div class="tab-pane" id="<%= tab.name %>">
			<p>tab</p>
			<% @questions.each do |question| %>
				<div>Question ID: <%= question.id %>, <%= question.question.html_safe %></div>
				<div><em><%= answer_for_question(question, @program, @year) %></em></div>
			<% end %>
		</div>
	<% end %>
	</div>
</div>
